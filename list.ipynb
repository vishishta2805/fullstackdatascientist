    "l=input().split()\n",
    "l1=list(map(int,l))\n",
    "p,n,z=0,0,0\n",
    "for i in l1:\n",
    "    if i>0:\n",
    "        p+=1\n",
    "    elif i<0:\n",
    "        n+=1\n",
    "    else:\n",
    "        z+=1\n",
    "print(f\"positive values:{p}, negative values:{n}, zeroes:{z}\")"
   ]
